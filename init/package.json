  "repository": "github:vendor/project",
  "scripts": {
    "db-upgrade": "node --experimental-modules ./bin/tequila.mjs db-schema-upgrade",
    "docs": "./node_modules/jsdoc/jsdoc.js --debug -p -c ./jsdoc.conf.json",
    "rebuild": "rm -fr node_modules package-lock.json && npm install",
    "start": "node --experimental-modules ./bin/tequila.mjs start",
    "stop": "node --experimental-modules ./bin/tequila.mjs stop",
    "test": "./node_modules/mocha/bin/mocha --require esm --recursive './test/**/*.test.js'"
  },
  "dependencies": {
    "@flancer32/teq_acl": "github:flancer32/teq_acl",
    "@flancer32/teq_user": "github:flancer32/teq_user",
    "@teqfw/core-app": "github:teqfw/core-app",
    "i18next": "*",
    "i18next-browser-languagedetector": "*",
    "marked": "*",
    "mime-types": "*",
    "mysql2": "*",
    "node-fetch": "*",
    "sanitize-html": "*",
    "serve-static": "*",
    "vue": "^3.0.1",
    "vue-router": "^4.0.0-rc.3",
    "vuex": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "jest": "*",
    "jsdoc": "*"
  },
  "teqfw": {
    "autoload": {
      "ns": "Vendor_Project",
      "path": "./src"
    }
  }
}
